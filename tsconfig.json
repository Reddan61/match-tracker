{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "paths": {
      "@/*": ["./src/*"],
      "svg/*": ["./public/assets/svg/*"],
      "fonts/*": ["./public/fonts/*"]
    }
  }
}
